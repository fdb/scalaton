<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>CFBundleDevelopmentRegion</key>
        <string>English</string>
        <key>CFBundleExecutable</key>
        <string>@@APP_NAME@@</string>
        <key>CFBundleIconFile</key>
        <string>@@APP_NAME@@.icns</string>
        <key>CFBundleInfoDictionaryVersion</key>
        <string>6.0</string>
        <key>CFBundleName</key>
        <string>@@APP_NAME@@</string>
        <key>CFBundlePackageType</key>
        <string>APPL</string>
        <key>CFBundleSignature</key>
        <string>????</string>
        <key>CFBundleGetInfoString</key>
        <string>@@APP_VERSION@@</string>
        <key>CFBundleShortVersionString</key>
        <string>@@APP_VERSION@@</string>
        <key>CFBundleVersion</key>
        <string>1</string>
        <key>Java</key>
        <dict>
            <key>ClassPath</key>
            <string>$APP_PACKAGE/Contents/Resources/Java/processing-core.jar:$APP_PACKAGE/Contents/Resources/Java/scala-library.jar:$APP_PACKAGE/Contents/Resources/Java/app.jar</string>
            <key>JVMVersion</key>
            <string>1.5*</string>
            <key>MainClass</key>
            <string>@@APP_MAIN_CLASS@@</string>
            <key>Properties</key>
            <dict>
                <key>com.apple.mrj.application.live-resize</key>
                <string>false</string>
                <key>apple.laf.useScreenMenuBar</key>
                <string>true</string>
                <key>com.apple.mrj.application.apple.menu.about.name</key>
                <string>@@APP_NAME@@</string>
                <key>apple.awt.graphics.UseQuartz</key>
                <string>true</string>
            </dict>
            <key>VMOptions</key>
            <string>-Djava.library.path=Java</string>
            <key>WorkingDirectory</key>
            <string>$APP_PACKAGE/Contents/Resources</string>
        </dict>
    </dict>
</plist>
